// Custom styles.

//20160917自定义滚动栏 custom_scrollbar 

::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
	border-radius: 10px;
	background-color: #F5F5F5;
}
::-webkit-scrollbar {
	width: 12px;
	background-color: #F5F5F5;
}
::-webkit-scrollbar-thumb {
	border-radius: 10px;
	-webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, .3);
	background-color: #555;
	background-image: -webkit-gradient(linear, 0% 100%, 0% 0%, from(#555), to(#555), color-stop(.5, #B6B6B6));
}
